<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.yb.dao.TeamDao">
	<select id="insertTeam">
		replace into worldcup_pk_team_event(id,name,logo,eventid) values
		<foreach collection="teams" index="i" separator=","  item="item">
			(#{item.id},#{item.name_zh},#{item.logo},#{item.matchevent_id})
		</foreach>
	</select>
	<select id="queryById" resultType="Team">
		select id,name,logo from worldcup_pk_team_event where id=#{id}
	</select>
</mapper>