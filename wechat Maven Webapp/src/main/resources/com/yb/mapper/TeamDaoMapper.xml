<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.yb.dao.TeamDao">
	<select id="insertTeam">
		replace into team_event(id,name,logo,eventid) values
		<foreach collection="teams" index="i" separator="," open="(" close=")" item="item">
			#{item.id},#{item.name_zh},#{item.logo},#{item.matchevent_id}
		</foreach>
	</select>
</mapper>